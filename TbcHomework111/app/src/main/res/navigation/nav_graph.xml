<navigation xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.tbchomework111.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_addNewDestinationFragment"
            app:destination="@id/addNewDestinationFragment" />
        <action
            android:id="@+id/action_homeFragment_to_editDestinationFragment"
            app:destination="@id/editDestinationFragment" />
    </fragment>
    <fragment
        android:id="@+id/addNewDestinationFragment"
        android:name="com.example.tbchomework111.AddNewDestinationFragment"
        android:label="fragment_add_new_destination"
        tools:layout="@layout/fragment_add_new_destination" />
    <fragment
        android:id="@+id/editDestinationFragment"
        android:name="com.example.tbchomework111.EditDestinationFragment"
        android:label="fragment_edit_destination"
        tools:layout="@layout/fragment_edit_destination" />
</navigation>
